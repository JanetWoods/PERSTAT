@model IEnumerable<PERSTAT.Models.States>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StateShort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StateName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StateShort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StateName)
            </td>
            
        </tr>
}
    </tbody>
</table>
